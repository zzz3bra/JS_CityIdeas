{
  "name": "angular2-cityideas",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"browser-sync start\" \"lite-server\" ",
    "docker-build": "docker build -t ng2-cityideas .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-cityideas",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server\" \"protractor protractor.config.js\"",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.1.0-rc.0",
    "@angular/compiler": "2.1.0-rc.0",
    "@angular/core": "2.1.0-rc.0",
    "@angular/forms": "2.1.0-rc.0",
    "@angular/http": "2.1.0-rc.0",
    "@angular/platform-browser": "2.1.0-rc.0",
    "@angular/platform-browser-dynamic": "2.1.0-rc.0",
    "@angular/router": "3.1.0-rc.0",
    "@angular/upgrade": "2.1.0-rc.0",
    "systemjs": "0.19.39",
    "core-js": "2.4.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.25",
    "bootstrap": "3.3.7"
  },
  "devDependencies": {
    "browser-sync":"2.17.2",
    "concurrently": "3.1.0",
    "lite-server": "2.2.2",
    "typescript": "2.0.3",
    "typings": "1.4.0",
    "canonical-path": "0.0.2",
    "http-server": "0.9.0",
    "tslint": "3.15.1",
    "lodash": "4.16.4",
    "jasmine-core": "~2.5.2",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "^0.3.8",
    "protractor": "^3.3.0",
    "rimraf": "2.5.4",
    "minimatch": "3.0.3"
  },
  "repository": {}
}